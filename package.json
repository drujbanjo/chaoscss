{
  "name": "chaoscss",
  "version": "1.1.1",
  "description": "ChaosCSS - is a free and simple open source CSS library.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Drugban/chaoscss"
  },
  "keywords": [
    "front-end",
    "framework",
    "css",
    "sass",
    "responsive"
  ],
  "main": "scss/chaos.scss",
  "author": "The ChaosCss authors (https://github.com/Drugban/chaoscss/graphs/contributors)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Drugban/chaoscss/issues"
  },
  "homepage": "chaoscss.vercel.app",
  "files": [
    "css/*",
    "scss/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier src/**/*.{ts,tsx,css,md,mdx,scss} --write",
    "format-all": "prettier . --write",
    "css-compile": "sass scss/chaos.scss:css/chaos.css",
    "css-minify": "sass scss/chaos.scss:css/chaos.min.css --style compressed"
  },
  "dependencies": {
    "@docsearch/react": "^3.6.1",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.5",
    "@types/mdx": "^2.0.13",
    "clsx": "^2.1.1",
    "highlight.js": "^11.10.0",
    "next": "14.2.5",
    "postcss": "^8.4.41",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-slug": "^6.0.0",
    "rehype-starry-night": "^2.1.0",
    "remark-directive": "^3.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "typescript": "^5.5.4"
  },
  "browserslist": [
    "> 1%"
  ]
}
